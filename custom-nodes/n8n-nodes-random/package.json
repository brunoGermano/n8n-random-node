  ],
  "license": "MIT",
  "author": "",
  "dependencies": {
    "n8n-core": "^1.14.1",
    "n8n-workflow": "^1.82.0"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "cpx": "^1.5.0",
    "typescript": "^5.9.2"
  }
}
