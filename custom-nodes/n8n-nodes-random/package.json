  "description": "Um conector n8n para gerar números aleatórios verdadeiros usando a API Random.org.",
  "main": "dist/Random.node.js",
  "scripts": {
    "build": "npm run copy-assets && tsc",
    "copy-assets": "cpx \"src/**/*.svg\" dist",
